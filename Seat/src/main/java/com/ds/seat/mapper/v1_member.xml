<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="v1_member">

	<select id="selectmemberlogin" resultType="V1_Member" parameterType="V1_Member">
		SELECT * FROM MEMBER WHERE M_ID=#{m_id} and M_PW=#{m_pw} and M_RETING=0 
	</select> 

</mapper>
